Stat.Name = "Stat"
Stat.Parent = Frame3
Stat.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
Stat.BorderSizePixel = 0
Stat.Position = UDim2.new(0.620000005, 0, 0.293962389, 0)
Stat.Size = UDim2.new(0, 156, 0, 31)
Stat.Font = Enum.Font.SourceSans
Stat.Text = "Kill Aura"
Stat.TextColor3 = Color3.fromRGB(255, 255, 255)
Stat.TextSize = 20.000
Stat.MouseButton1Down:connect(function()
	local plr = game.Players.LocalPlayer
	local char = plr.Character

	while wait() do
		for i,v in next, game:service'Players':GetPlayers() do
			if v ~= plr then
				local distance = v:DistanceFromCharacter(char.HumanoidRootPart.Position)
				if distance < 20 then
					for a,x in pairs(char:GetChildren())do
						if x:IsA("Tool") then
							spawn(function()
								x.RemoteEvent:FireServer("Attack", v.Character.Humanoid)
							end)
						end
					end
				end
			end
		end
	end
end)

