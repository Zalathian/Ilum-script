-- Scripts:

local function MISQU_fake_script() -- IlumGui.WL 
	local script = Instance.new('LocalScript', IlumGui)

	
	local Frame3 = script.Parent.Frame3
	local IBlocks = script.Parent.Frame3.IBlocks
	local IForce = script.Parent.Frame3.IForce	
	local Medi = script.Parent.Frame3.Medi
	local TK = script.Parent.Frame3.TK
	local ABlocks = script.Parent.Frame3.ABlocks
	local Stat = script.Parent.Frame3.Stat
	local hotkey = Enum.KeyCode.Insert
	local F4 = script.Parent.Frame3.F4
	local cred2 = script.Parent.Frame3.cred2
	local cred = script.Parent.Frame3.cred2
	Frame3.Visible = true
	Frame3.Draggable = true
	
	
	local UIS = game:GetService("UserInputService")
	local open = false
	UIS.InputBegan:Connect(function(key, gp)
				if key.KeyCode == hotkey then
					Frame3.Visible = true
			if open == false then
						open = true
						Frame3:TweenPosition(
							UDim2.new(0.392242986, 0, 0.348, 0),
							"InOut",
							"Quad",
							1.1,
							false
						)
				Frame3.Visible = open
			elseif open == true then
						open = false
						Frame3:TweenPosition(
							UDim2.new(0.392242986, 0, 1.5, 0),
							"InOut",
							"Quad",
							1.1,
							false
							)
						wait(2)
				Frame3.Visible = open
			end	
		end
	end)
end
coroutine.wrap(MISQU_fake_script)()
