Button3.TextColor3 = Color3.fromRGB(255, 255, 255)
Button3.TextSize = 20.000
Button3.MouseButton1Down:connect(function()

	local A_1 = "Double"
	local A_2 = "Lightsaber"
	local A_3 = 
		{
		[1] = 0, 
		[2] = 0
	}
	local Event = game:GetService("ReplicatedStorage").Remotes.GiveLoadouts
	Event:InvokeServer(A_1, A_2, A_3)
	local kill = game.Players.LocalPlayer.Character
	kill.Torso:Destroy()
	kill.Head:Destroy()
	kill.LeftArm:Destroy()
	kill.RightArm:Destroy()
	kill.LeftLeg:Destroy()
	kill.RightLeg:Destroy()
end)
