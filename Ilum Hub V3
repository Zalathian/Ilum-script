IForce.Text = "Infinite Force"
IForce.TextColor3 = Color3.fromRGB(255, 255, 255)
IForce.TextSize = 20.000
IForce.MouseButton1Down:connect(function()
	getgenv().force = true
	if getgenv().force == true then
		local A_1000 = "Dash"
		local A_1002 = 
			{
			[1] = game:GetService("Players").LocalPlayer
		}
		local A_1003 = -9999999999999999999999999999
		local A_1004 = true
		local Event = game:GetService("ReplicatedStorage").Remotes.ForcePower
		Event:FireServer(A_1000, A_1002, A_1003, A_1004)
	end
end)

IBlocks.Name = "IBlocks"
IBlocks.Parent = Frame3
IBlocks.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
IBlocks.BorderColor3 = Color3.fromRGB(27, 42, 53)
IBlocks.BorderSizePixel = 0
IBlocks.Position = UDim2.new(0.620000005, 0, 0.409684211, 0)
IBlocks.Size = UDim2.new(0, 156, 0, 31)
IBlocks.Font = Enum.Font.SourceSans
IBlocks.Text = "Infinite Blocks"
IBlocks.TextColor3 = Color3.fromRGB(255, 255, 255)
IBlocks.TextSize = 20.000
IBlocks.MouseButton1Down:connect(function()
	getgenv().iBlocks = true
	while wait() do
		if getgenv().iBlocks == true then
			game.Players.LocalPlayer.PlayerGui.StatsGui.Block.Count.Text = 100
		end
	end
end)

ABlocks.Name = "ABlocks"
ABlocks.Parent = Frame3
ABlocks.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
ABlocks.BorderSizePixel = 0
ABlocks.Position = UDim2.new(0.620000005, 0, 0.754330873, 0)
ABlocks.Size = UDim2.new(0, 156, 0, 31)
ABlocks.Font = Enum.Font.SourceSans
ABlocks.Text = "Auto Block"
ABlocks.TextColor3 = Color3.fromRGB(255, 255, 255)
ABlocks.TextSize = 20.000
ABlocks.MouseButton1Down:connect(function()
	local Ployth = game.Players.LocalPlayer
	local Avatar = Ployth.Character
	getgenv().Autoblock = true;
	while wait() do
		if getgenv().Autoblock == true then
			for i,v in next, game:service'Players':GetPlayers() do
				if v ~= Ployth then
					for a,x in pairs(Avatar:GetChildren())do
						if x:IsA("Tool") then
							spawn(function()
								x.RemoteEvent:FireServer("Block", v.Character.Humanoid)
							end)
						end
					end
				end
			end
		end
	end
end)

